# Tested Versions - Settlers_v1.5_0143
# Increased fastmem_size to handle memory requirements
use_gui=no
show_leds=true
chipset=ecs
cpu_type=68000
chipmem_size=4
fastmem_size=8
gfx_center_vertical=simple
